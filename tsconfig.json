{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "*": ["/*"],
      "components/*": ["components/*"],
      "library/*": ["library/*"],
      "utils/*": ["utils/*"],
      "pages/*": ["pages/*"],
      "configs/*": ["configs/*"],
      "interfaces/*": ["interfaces/*"],
      "public/*": ["public/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "configs/my-account.js", "components/layouts/shared/section.tsx", "components/layouts/shared/header.tsx"],
  "exclude": ["node_modules"]
}
